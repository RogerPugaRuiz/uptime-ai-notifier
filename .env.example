# N8N Configuration for Production
# Copy this file to .env and configure with your values

# Host where n8n is accessible
N8N_HOST=localhost

# Protocol (http or https)
# Note: Use https only if you have SSL certificates configured
N8N_PROTOCOL=http

# Webhook URL for n8n
# Note: Update to https if you have SSL certificates configured
WEBHOOK_URL=http://localhost:5678/

# Timezone configuration
TIMEZONE=UTC
